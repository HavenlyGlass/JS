let quantity = Math.round(Math.random() * 100); //кол-во товаров
let n;
let word1 = 'товаров'; //слово
let word2 = 'товара';
let word3 = 'товар';

n = quantity % 10

console.log(n)


if (n >= 5 && n <= 9 || n == 0) {
    console.log(`${quantity} ${word1}`);
}
if (n >= 2 && n <=4) {
    console.log(`${quantity} ${word2}`);
}
if (n == 1) {
    console.log(`${quantity} ${word3}`);
}  
